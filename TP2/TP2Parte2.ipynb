    "# Ejercicio 3\n",
    "\n",
    "from dbfread import DBF\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Archivos .dbf ,
    "archivo_2004 = 'usu_individual_1t2004.dbf'\n",
    "archivo_2024 = 'usu_individual_1t2024.dbf'  # si no tenés 2024, usá 2023\n",
    "\n",
    "# Leer bases\n",
    "data_2004 = pd.DataFrame(DBF(archivo_2004, load=True))\n",
    "data_2024 = pd.DataFrame(DBF(archivo_2024, load=True))\n",
    "\n",
    "# Código de aglomerados para provincias del NOA\n",
    "noroeste_codigos = [6, 7, 9, 10, 18, 19]  # Catamarca, Tucumán, Jujuy, La Rioja, Salta, Santiago del Estero\n",
    "\n",
    "# Filtrar por región NOA\n",
    "data_2004_noa = data_2004[data_2004['AGLOMERADO'].isin(noroeste_codigos)]\n",
    "data_2024_noa = data_2024[data_2024['AGLOMERADO'].isin(noroeste_codigos)]\n",
    "\n",
    "# Variable CH04: sexo (1 = varón, 2 = mujer)\n",
    "sexo_2004 = data_2004_noa['CH04'].value_counts().sort_index()\n",
    "sexo_2024 = data_2024_noa['CH04'].value_counts().sort_index()\n",
    "\n",
    "# Crear DataFrame combinado\n",
    "df = pd.DataFrame({\n",
    "    '2004': sexo_2004,\n",
    "    '2024': sexo_2024\n",
    "})\n",
    "df.index = ['Hombres', 'Mujeres']\n",
    "\n",
    "# Gráfico de barras\n",
    "ax = df.plot(kind='bar', figsize=(8,6), color=['#1f77b4', '#ff7f0e'])\n",
    "\n",
    "plt.title('Composición por sexo en el Noroeste Argentino (2004 vs 2024)')\n",
    "plt.xlabel('Sexo')\n",
    "plt.ylabel('Cantidad de personas')\n",
    "plt.xticks(rotation=0)\n",
    "plt.grid(axis='y', linestyle='--', alpha=0.7)\n",
    "plt.legend(title='Año')\n",
    "plt.tight_layout()\n",
    "\n",
    "plt.show()"
   ]
