    "# Ejercicio 4\n",
    "\n",
    "# Importación de librerías necesarias\n",
    "from dbfread import DBF\n",
    "import pandas as pd\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# --------------------------------------------\n",
    "# CARGA DE DATOS .DBF DESDE ARCHIVOS DEL INDEC\n",
    "# --------------------------------------------\n",
    "\n",
    "# Cambiar estos nombres si bajaste otro trimestre\n",
    "archivo_2004 = 'usu_individual_1t2004.dbf'\n",
    "archivo_2024 = 'usu_individual_1t2024.dbf'\n",
    "\n",
    "# Leer los archivos DBF\n",
    "data_2004 = pd.DataFrame(DBF(archivo_2004, load=True))\n",
    "data_2024 = pd.DataFrame(DBF(archivo_2024, load=True))\n",
    "\n",
    "# --------------------------------------------\n",
    "# SELECCIÓN DE VARIABLES RELEVANTES\n",
    "# --------------------------------------------\n",
    "\n",
    "# Variables solicitadas: 'CH04', 'CH06', 'CH07', 'CH08', 'NIVEL ED', 'ESTADO', 'CAT_INAC', 'IPCF'\n",
    "columnas_seleccionadas = ['CH04', 'CH06', 'CH07', 'CH08', 'NIVEL ED', 'ESTADO', 'CAT_INAC', 'IPCF']\n",
    "\n",
    "# Filtrar los datos solo con esas columnas\n",
    "data_2004 = data_2004[columnas_seleccionadas]\n",
    "data_2024 = data_2024[columnas_seleccionadas]\n",
    "\n",
    "# --------------------------------------------\n",
    "# CÁLCULO DE LA MATRIZ DE CORRELACIÓN\n",
    "# --------------------------------------------\n",
    "\n",
    "# Calcular la matriz de correlación para 2004 y 2024\n",
    "corr_2004 = data_2004.corr()\n",
    "corr_2024 = data_2024.corr()\n",
    "\n",
    "# --------------------------------------------\n",
    "# VISUALIZACIÓN DE LA MATRIZ DE CORRELACIÓN (HEATMAP)\n",
    "# --------------------------------------------\n",
    "\n",
    "# Configuración de tamaño del gráfico\n",
    "plt.figure(figsize=(14, 8))\n",
    "\n",
    "# Crear el heatmap para 2004\n",
    "plt.subplot(1, 2, 1)\n",
    "sns.heatmap(corr_2004, annot=True, cmap='coolwarm', fmt='.2f', linewidths=0.5, vmin=-1, vmax=1)\n",
    "plt.title('Matriz de Correlación 2004')\n",
    "\n",
    "# Crear el heatmap para 2024\n",
    "plt.subplot(1, 2, 2)\n",
    "sns.heatmap(corr_2024, annot=True, cmap='coolwarm', fmt='.2f', linewidths=0.5, vmin=-1, vmax=1)\n",
    "plt.title('Matriz de Correlación 2024')\n",
    "\n",
    "# Mostrar ambos heatmaps\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    " \n",
    "# \"Observamos que para el año 2004, la variable 'CH04' (sexo) tiene una correlación positiva\n",
    "# con 'CH06' (edad) y una correlación negativa con 'CAT_INAC' (categoría de inactividad). \n",
    "# Esto sugiere que la mayor parte de los hombres y mujeres están dentro de un grupo de edad\n",
    "# más joven, mientras que las personas inactivas presentan una correlación negativa.\""
   ]
  }
